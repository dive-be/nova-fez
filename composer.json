{
    "name": "dive-be/nova-fez",
    "description": "A Laravel Nova field.",
    "keywords": [
        "fez",
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/dive-be/nova-fez",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed Sari",
            "email": "muhammed@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "dive-be/laravel-fez": "^0.2",
        "laravel/nova": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.20",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dive\\Fez\\Nova\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dive\\Fez\\Nova\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
